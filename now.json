{
  "version": 2,
  "name": "cs411-exercise-tracker",
  "alias": "cs411-exercise-tracker.now.sh",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/asset/images/(.*)",
      "headers": {
        "cache-control": "s-maxage=604800"
      },
      "dest": "client/asset/images/$1"
    },
    {
      "src": "/asset/icon/(.*)",
      "headers": {
        "cache-control": "s-maxage=604800"
      },
      "dest": "client/asset/icon/$1"
    },
    {
      "src": "/asset/manifest.json",
      "dest": "client/asset/manifest.json"
    },
    {
      "src": "/client/src/service-worker.js",
      "headers": {
        "cache-control": "s-maxage=0"
      },
      "dest": "client/service-worker.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html"
    }
  ]
}
